<HBox id="credentialsPane" fx:id="credentialsPane" alignment="TOP_CENTER" stylesheets="@../styling/credentials.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CredentialsController">
