<HBox fx:id="rootPane" minHeight="600.0" stylesheets="@../styling/main_styling.css"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.MainController">
	<children>
		<VBox prefHeight="200.0" prefWidth="100.0">
			<children>
				<StackPane fx:id="contentPane" alignment="TOP_LEFT"
					VBox.vgrow="ALWAYS">
					<children>
						<fx:include fx:id="storePane" source="store.fxml" />
						<fx:include fx:id="loadListPane" source="load_list.fxml" />
						<AnchorPane fx:id="welcomePane" disable="true"
							prefHeight="200.0" prefWidth="200.0" visible="false">
							<StackPane.margin>
								<Insets left="14.0" top="14.0" />
							</StackPane.margin>
							<children>
								<VBox alignment="CENTER" prefHeight="170.0" prefWidth="636.0"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
									<children>
										<Label text="Välkommen!">
											<font>
												<Font size="64.0" />
											</font>
										</Label>
									</children>
								</VBox>
							</children>
						</AnchorPane>
					</children>
				</StackPane>
				<AnchorPane prefHeight="60.0">
					<children>
						<Button fx:id="nextButton" layoutX="485.0" layoutY="15.0"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="135.0"
							text="Gå till kassan" AnchorPane.bottomAnchor="14.0"
							AnchorPane.rightAnchor="7.0" AnchorPane.topAnchor="0.0" />
						<Button fx:id="prevButton" layoutX="29.0" layoutY="15.0"
							minWidth="-Infinity" mnemonicParsing="false" prefWidth="135.0"
							text="Gå till startsidan" AnchorPane.bottomAnchor="14.0"
							AnchorPane.leftAnchor="7.0" AnchorPane.topAnchor="0.0" />
					</children>
				</AnchorPane>
			</children>
		</VBox>
		<fx:include fx:id="shoppingCart" source="shopping_cart.fxml" />
	</children>
</HBox>
