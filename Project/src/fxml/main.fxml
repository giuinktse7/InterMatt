<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import control.ArrowButton?>
<?import control.NavigationButton?>

<StackPane fx:id="wrapperStackPane" stylesheets="@../styling/root.css"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.MainController">
	<children>
		<fx:include fx:id="purchaseHistoryPopup" maxWidth="-Infinity"
			maxHeight="-Infinity" source="purchase_history.fxml" />
		<fx:include fx:id="loadListPopup" maxWidth="-Infinity"
			maxHeight="-Infinity" source="load_list.fxml" />
		<fx:include fx:id="saveListPopup" maxWidth="-Infinity"
			maxHeight="-Infinity" source="save_list.fxml" />
		<fx:include fx:id="recipePopup" maxWidth="-Infinity"
			maxHeight="-Infinity" source="recipe.fxml" />
		<VBox fx:id="mainContentWrapper">
			<children>
				<SplitPane fx:id="splitPane" dividerPositions="0.75"
					prefHeight="700.0" prefWidth="1000.0" styleClass="backdrop"
					VBox.vgrow="ALWAYS">
					<items>
						<AnchorPane minWidth="1000.0">
							<children>
								<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
									<children>
										<StackPane fx:id="contentPane" alignment="TOP_LEFT"
											VBox.vgrow="ALWAYS">
											<children>
												<fx:include fx:id="purchasePane" source="purchase.fxml" />
												<fx:include fx:id="credentialsPane" source="credentials.fxml" />
												<fx:include fx:id="storePane" source="store.fxml" />
											</children>
										</StackPane>
									</children>
								</VBox>
							</children>
						</AnchorPane>
						<AnchorPane prefHeight="601.0" prefWidth="142.0">
							<children>
									<Button maxWidth="-Infinity" HBox.hgrow="ALWAYS" fx:id="purchaseHistoryButton" mnemonicParsing="false"
										prefHeight="36" text="Köphistorik" AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0" styleClass="history-button"
									AnchorPane.rightAnchor="0">
									</Button>
								<fx:include fx:id="shoppingCart" source="shopping_cart.fxml" />
							</children>
						</AnchorPane>
					</items>
				</SplitPane>
				<AnchorPane prefHeight="103.0" prefWidth="1057.0"
					styleClass="base-style">
					<children>
						<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
							<children>

								<ArrowButton fx:id="prevButton" minWidth="-Infinity"
									mnemonicParsing="false" styleClass="nav-button">
									<graphic>
										<ImageView fitWidth="90.0" pickOnBounds="true"
											preserveRatio="true">
											<image>
												<Image url="@../resources/pilVänster.png" />
											</image>
										</ImageView>
									</graphic>
									<HBox.margin>
										<Insets right="28.0" />
									</HBox.margin>
								</ArrowButton>
								<NavigationButton fx:id="btnToStore" minHeight="60.0"
									minWidth="60.0" mnemonicParsing="false" prefHeight="60.0"
									prefWidth="60.0" styleClass="nav-button">
									<graphic>
										<ImageView fitHeight="80.0" fitWidth="80.0"
											pickOnBounds="true" preserveRatio="true">
											<image>
												<Image url="@../resources/previous1.png" />
											</image>
										</ImageView>
									</graphic>
									<HBox.margin>
										<Insets right="28.0" />
									</HBox.margin>
								</NavigationButton>
								<NavigationButton fx:id="btnToCredentials"
									disable="true" minHeight="64.0" minWidth="64.0"
									mnemonicParsing="false" prefHeight="64.0" prefWidth="64.0"
									styleClass="nav-button">
									<graphic>
										<ImageView fitHeight="80.0" fitWidth="80.0"
											pickOnBounds="true" preserveRatio="true">
											<image>
												<Image url="@../resources/thisPage2.png" />
											</image>
										</ImageView>
									</graphic>
									<HBox.margin>
										<Insets right="28.0" />
									</HBox.margin>
								</NavigationButton>
								<NavigationButton fx:id="btnToPurchase"
									disable="true" minHeight="60.0" minWidth="60.0"
									mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0"
									styleClass="nav-button">
									<graphic>
										<ImageView disable="true" fitHeight="80.0" fitWidth="80.0"
											pickOnBounds="true" preserveRatio="true">
											<image>
												<Image url="@../resources/disabled3.png" />
											</image>
										</ImageView>
									</graphic>
									<HBox.margin>
										<Insets right="28.0" />
									</HBox.margin>
								</NavigationButton>
								<NavigationButton fx:id="navButton4" disable="true"
									minHeight="60.0" minWidth="60.0" mnemonicParsing="false"
									prefHeight="60.0" prefWidth="60.0" styleClass="nav-button">
									<graphic>
										<ImageView fitHeight="80.0" fitWidth="80.0"
											pickOnBounds="true" preserveRatio="true">
											<image>
												<Image url="@../resources/disabled4.png" />
											</image>
										</ImageView>
									</graphic>
									<HBox.margin>
										<Insets right="28.0" />
									</HBox.margin>
								</NavigationButton>
								<ArrowButton fx:id="nextButton" minWidth="-Infinity"
									mnemonicParsing="false" styleClass="nav-button">
									<graphic>
										<ImageView fitWidth="90.0" pickOnBounds="true"
											preserveRatio="true">
											<image>
												<Image url="@../resources/pilHöger.png" />
											</image>
										</ImageView>
									</graphic>
									<HBox.margin>
										<Insets right="14.0" />
									</HBox.margin>
								</ArrowButton>
							</children>
						</HBox>
					</children>
				</AnchorPane>
			</children>
		</VBox>
	</children>
</StackPane>
